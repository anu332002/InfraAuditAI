{
  "dashboard": {
    "id": null,
    "uid": "example-dashboard",
    "title": "Example Dashboard",
    "tags": [],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "panels": [
      {
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "target": "avg(rate(node_cpu_seconds_total{mode='idle'}[5m])) by (instance)",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "percent",
            "label": "CPU Usage",
            "min": 0,
            "max": 1
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "target": "avg(node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes) by (instance)",
            "refId": "B"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "percent",
            "label": "Memory Usage",
            "min": 0,
            "max": 1
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "refresh": "5s"
  }
}